pipeline{
    agent any
    stages{
        stage('In√≠cio'){
            steps{
                sh 'echo iniciooooooooooooo'
                sh 'pwd'
            }
        }
        stage('MEIO'){
            steps{
                sh 'echo meioooooooooooooooooooooooooooo'
                sh 'pwd'
            }
        }
        stage('FIM'){
            steps{
                sleep(5)
                sh 'echo meioooooooooooooooooooooooooooo'
                sh 'pwd'
            }
        }
        
    }
}
